{
  "name": "ambos",
  "description": "Advanced Multi-source Biosecurity OSINT System - Intelligence platform for defense and security",
  "version": "2.0.0",
  "private": false,
  "type": "module",
  "author": "Your Organization",
  "license": "SEE LICENSE IN LICENSING.md",
  "homepage": "https://ambos.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ambos.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ambos/issues",
    "email": "support@ambos.dev"
  },
  "keywords": [
    "osint",
    "intelligence",
    "defense",
    "security",
    "military",
    "threat-intelligence",
    "ai-analysis",
    "geospatial",
    "news-aggregation"
  ],
  "packageManager": "npm@10.9.3",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "migrate": "bash scripts/migrate-from-lovable.sh",
    "replace-ai": "node scripts/replace-ai-provider.js",
    "docker:build": "docker build -t ambos:latest .",
    "docker:run": "docker run -p 3000:80 ambos:latest",
    "supabase:start": "cd supabase && npx supabase start",
    "supabase:stop": "cd supabase && npx supabase stop",
    "supabase:push": "cd supabase && npx supabase db push",
    "supabase:deploy": "cd supabase && npx supabase functions deploy",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\""
  },
